{
  "name": "sztyup/pdf",
  "description": "PDF importing/exporting made easier",
  "license": "LGPL-3.0",
  "authors": [{
    "name": "Csupity László",
    "email": "sztyup@gmail.com"
  }],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^5.6",
    "tecnickcom/tcpdf": "^6.2",
    "setasign/fpdi": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "orchestra/testbench": "~3.0",
    "mockery/mockery": "^1.0"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Sztyup\\PDF\\": "src/Pdf"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sztyup\\Pdf\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sztyup\\Pdf\\PdfServiceProvider"
      ]
    }
  }
}
